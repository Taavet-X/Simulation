import numpy as np
#a = -1/lambda * ln(r)
# r es un aleatori en (0,1]
#lambda es un margen de error

rs = [0.5520, 0.4881, 0.7515, 0.3124, 0.5696, 0.7238, 0.9438]

#distribucion exponencial
l = 1.5
print("")
for r in rs:
    a = -1/l * np.log(r)
    print(a)


#acomulada
#a = (B-A)r+A
A = 10
B = 20
print("Acomulada")
for r in rs:
    a = (B-A)*r+A
    print(a)

#normal estandarizada

